
.socials__list{
    display: flex;
    align-items: center;
}

.socials__item{
    margin-right: 40px;
    width: 25px;
    height: 31px;
    display: block;
    &-vk {
        width: 31px;
    }
    &-github {
        width: 27px;
    }
    &:last-child{
        margin-right: 0;
    }
}

.socials__link{
}
.socials__icon{
    fill: #414c63;
    &:hover {
        fill: #5500f2;
    }
}